{
  "name": "Twimemo",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Test",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [{
    "matches": ["https://twitter.com/*"],
    "css": [
      "flatpickr.min.css",
      "style.css"
    ]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
